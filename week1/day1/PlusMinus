def plusMinus(arr):
    positive=[]
    negative=[]
    zero=[]
    size=len(arr)
    for item in arr:
        if item>0:
            positive.append(item)
        elif item<0:
            negative.append(item)
        else:
            zero.append(item)
    p=len(positive)/size
    n=len(negative)/size
    z=len(zero)/size
    print(round(p,6))
    print(round(n,6))
    print(round(z,6))

